<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">
    <record id="ir_cron_sync_discipline_manager_group" model="ir.cron">
      <field name="name">Discipline: Sync Manager Group</field>
      <field name="model_id" ref="hr.model_hr_employee"/>
      <field name="state">code</field>
      <field name="code">model._cron_sync_discipline_manager_group()</field>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="active">True</field>
      <field name="nextcall"
             eval="(datetime.now() + relativedelta(days=1, hour=6, minute=0, second=0)).strftime('%Y-%m-%d %H:%M:%S')"/>
      <field name="user_id" ref="base.user_root"/>
    </record>
  </data>
</odoo>
