        <field name="name">Download Skipped/Not Found Products Report</field>
        <field name="model_id" ref="otd_marketplace_stock.model_marketplace_account"/>
        <field name="binding_model_id" ref="otd_marketplace_stock.model_marketplace_account"/>
        <field name="binding_view_types">form</field>
        <field name="state">code</field>
        <field name="code">
action = record.action_download_skipped_products()
        </field>
    </record>
</odoo>

